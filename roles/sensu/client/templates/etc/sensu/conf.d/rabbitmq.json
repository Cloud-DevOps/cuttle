{
  "rabbitmq": {
    "ssl": {
      "cert_chain_file": "/etc/sensu/ssl/cert.pem",
      "private_key_file": "/etc/sensu/ssl/key.pem"
    },
    "host": "{{ sensu.client.rabbitmq.host }}",
    "port": 5671,
    "vhost": "{{ sensu.client.rabbitmq.vhost }}",
    "user": "{{ sensu.client.rabbitmq.username }}",
    "password": "{{ sensu.client.rabbitmq.password }}"
  }
}
